#include<stdio.h>
#include<stdlib.h> 
int main() {
    int x, y;
    do{

        printf ("informe o valor 25\n");
        scanf ("%d",&x);
        if (x!=25) {
            printf ("numero errado, o valor deve ser igual a 25! \n");
        }
    }while (x!=25)
    y=&x;
    *y=12;
    y+=1;
    *y+=5;
    printf("\t O valor de x eh: %d\n\n", x);
    retorn 0;
}  